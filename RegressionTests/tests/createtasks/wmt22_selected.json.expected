        "itemID": 5,
        "itemType": "TGT",
        "sourceContextLeft": "",
        "sourceID": "newstest2021.de-fr.example.xml",
        "sourceText": "Anmeldungen",
        "targetContextLeft": "",
        "targetID": "Online-B",
        "targetText": "Inscriptions"
      },
      {
