        "isCompleteDocument": false,
        "itemID": 9,
        "itemType": "TGT",
        "sourceContextLeft": "",
        "sourceID": "newstest2021.de-fr.example.xml",
        "sourceText": "Ladeinfrastruktur:Auch die wird gef\u00f6rdert.",
        "targetContextLeft": "",
        "targetID": "Online-A",
        "targetText": "Infrastructure de recharge: elle est \u00e9galement encourag\u00e9e."
